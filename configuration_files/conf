#test comment

server
{
	server_name exemple.fr;
	listen 3200
	error_page 404 /custom_404.html
	client_max_body_size 1000000
	
	location /index
	{
		allow_method GET POST DELETE
		return 301 /index/error
		root /root
		index /index/1
		auto_index on
		cgi .php ./www/bin/php-cgi
		upload /home/upload
	}

}

